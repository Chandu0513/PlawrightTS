{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "types": ["node", "@playwright/test"],
    "outDir": "dist"
  },
  "include": ["tests", "pages", "utils", "configs", "ai-agents", "data", "playwright.config.ts"]
}
